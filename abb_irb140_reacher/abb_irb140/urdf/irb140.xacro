	<!-- tcp link -->
	<link name="tcp_link" />
	<joint name="tcp_joint" type="fixed">
		<parent link="link_6" />
		<child link="tcp_link" />
		<origin rpy="0 0 0" xyz="0.0 0 0" />
	</joint>

	<!-- Used for fixing robot to Gazebo 'base_link' -->
	<link name="world" />
