	<xacro:arg name="robot_namespace" default="/" />
	<!-- Macros de links y joints generalizados -->
	<xacro:include filename="$(find abb_irb140)/urdf/irb140_joints.xacro" />
	<!-- Import all Gazebo-customization elements, including Gazebo colors -->
	<xacro:include filename="$(find abb_irb140)/urdf/irb140.gazebo" />

	<!--LINKS-->
	<!--base_link-->
	<link name="base_link">
		<visual>
			<origin rpy="0 0 0" xyz="0 0 0" />
			<geometry>
				<mesh filename="package://abb_irb140/meshes/visual/base_link.stl" scale="0.01 0.01 0.01" />
			</geometry>
			<material name="abb_orange">
				<color rgba="1 0.43 0 1" />
			</material>
		</visual>
		<collision>
			<origin rpy="0 0 0" xyz="0 0 0" />
			<geometry>
				<mesh filename="package://abb_irb140/meshes/collision_low_poly/base_link.stl" scale="0.01 0.01 0.01" />
			</geometry>
			<material name="yellow">
				<color rgba="0 1 1 1" />
			</material>
		</collision>
		<inertial>
			<mass value="26.36" />
			<origin xyz="-0.0820 0.0004 0.0599" rpy="0 0 0" />
			<inertia ixx="0.222" ixy="0" ixz="0.0156" iyy="0.3553" iyz="0.0017" izz="0.5236" />
		</inertial>
	</link>
